{
  "exclude": ["node_modules", "build"],
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noImplicitAny": false, //
    "checkJs": false,//
    "allowJs": true,//
    "skipLibCheck": true,
    "declaration": true,//
    "emitDeclarationOnly": true,//
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    // "module": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "removeComments": true
  }
}
